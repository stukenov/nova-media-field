{
  "name": "stukenov/nova-media-field",
  "description": "Laravel Nova field for image/gallery upload and library browsing.",
  "keywords": [
    "laravel",
    "nova",
    "stukenov",
    "media"
  ],
  "authors": [
    {
      "name": "Marttin Notta",
      "email": "marttinnotta@stukenov.com",
      "role": "Developer"
    },
    {
      "name": "Optimist Digital",
      "email": "info@stukenov.com",
      "role": "Maintainer"
    }
  ],
  "license": "MIT",
  "require": {
    "php": ">=8.0.0",
    "intervention/image": "^2.7.1",
    "laravel/nova": "~4.35",
    "php-ffmpeg/php-ffmpeg": "^1.0.1",
    "guzzlehttp/guzzle": "^7.9.2"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://nova.laravel.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "STukenov\\MediaField\\": [
        "src/"
      ]
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "STukenov\\MediaField\\FieldServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require-dev": {
    "rector/rector": "^2.0"
  }
}
